<h1>All Songs</h1>

<table>
<thead>
    <tr>
        <th>Title</th>
        <th>Released</th>
        <th>Release Year</th>
        <th>Artist</th>
        <th>Genre</th>
    </tr>
</thead>
<tbody><% @songs.each do |s| %>
    <tr>
        <td><%= link_to s.title, song_path(s) %></td>
        <td><%= s.released %></td>
        <td><%= s.release_year %></td>
        <td><%= s.artist_name %></td>
        <td><%= s.genre %></td>
        <% end %>
    </tr>
</tbody>
</table>
